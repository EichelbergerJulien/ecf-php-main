{% extends "layout.html.twig" %}

{% block title %}
Auteurs
{% endblock title %}




{% block main %}
<main>
    <table class="table">
        <tr>
            <th scope="col">
                <h2>Auteurs</h2>
            </th>
        </tr>
        {% for auteur in auteurs %}
        <tr>
            <td>{{ auteur.prenom }} {{ auteur.nom }}</td>
        </tr>
        {% endfor %}
    </table>
</main>
{% endblock %}